{
    "name": "emekaorjiani/linkio",
    "description": "Laravel package for LinkIO integration",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EmekaOrjiani\\LinkIO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmekaOrjiani\\LinkIO\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
          "providers": [
            "EmekaOrjiani\\LinkIO\\LinkIOServiceProvider"
          ],
          "aliases": {
            "LinkIO": "EmekaOrjiani\\LinkIO\\Facades\\LinkIO"
          }
        }
    },
    "authors": [
        {
            "name": "DoctorVee Orjiani",
            "email": "emekaorjiani@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^10.1"
    }
}
